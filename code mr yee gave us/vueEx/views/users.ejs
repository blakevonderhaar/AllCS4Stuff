<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>

    	<% users.forEach(function(user){ %>
    		<li><%= user.lastName %>, <%= user.firstName %></li>
    	<%	}); %>

    	<% for(var i=0;i<11;i++){
    	if(i%2 != 1){	%>
    	<li><%=i%></li>
    	<% }} %>
    	
    	<% for(var i=0;i<11;i++){
    	if(i%2 == 1){	%>
    	<li><%=i%></li>
    	<% }} %>

    </ul>
  </body>
</html>
